{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adflab5005"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1'"
		},
		"AzureSynapseAnalytics1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureSynapseAnalytics1'"
		},
		"AzureSynapseAnalytics2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics2'"
		},
		"AzureSynapseAnalytics2_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureSynapseAnalytics2'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlslab5005.dfs.core.windows.net"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "75054167-4f5f-4215-ba98-fe94de9993f0"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "12eb616c-9191-49a0-acc9-664bc577c6a2"
		},
		"AzureSynapseAnalytics2_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "75054167-4f5f-4215-ba98-fe94de9993f0"
		},
		"AzureSynapseAnalytics2_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "12eb616c-9191-49a0-acc9-664bc577c6a2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adflab5005_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"AzureSynapseAnalytics1_servicePrincipalKey": {
						"value": "[parameters('AzureSynapseAnalytics1_servicePrincipalKey')]"
					},
					"AzureSynapseAnalytics2_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics2_connectionString')]"
					},
					"AzureSynapseAnalytics2_servicePrincipalKey": {
						"value": "[parameters('AzureSynapseAnalytics2_servicePrincipalKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_tenant')]"
					},
					"AzureSynapseAnalytics2_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureSynapseAnalytics2_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureSynapseAnalytics2_properties_typeProperties_tenant": {
						"value": "[parameters('AzureSynapseAnalytics2_properties_typeProperties_tenant')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}